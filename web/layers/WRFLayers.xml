		<Menu ID="co" EN="CO" ES="CO"/> 
		<Menu ID="o3" EN="O3" ES="O3"/>
		<Menu ID="PM10" EN="Hourly Precipitation " ES="Precipitacion Horaria"/>
                
                <Menu ID="so2" EN="Cloud Cover IR" ES="Cobertura Nubosa IR" />
