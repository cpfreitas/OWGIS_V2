            <layer Menu="Dom2,Prec,PrecTot" EN="Total Precipitation" ES="Precipitacion Total" style="default-scalar" palette="prec2" mincolor="0" maxcolor="400" name="wrf_dom2/PREC2"  />

            <layer Menu="Dom2,Prec,Prec3h" EN="Precipitation Acumulated each 3 hours" ES="Precipitacion Acumulada cada 3 horas." style="default-scalar" palette="prec2" mincolor="0" maxcolor="400" name="wrf_dom2/PRECA3"  />

            <layer Menu="Dom2,Prec,Prec6h" EN="Precipitation Acumulated each 6 hours" ES="Precipitacion Acumulada cada 6 horas." style="default-scalar" palette="prec2" mincolor="0" maxcolor="400" name="wrf_dom2/PRECA6"  />
            
            <layer Menu="Dom2,Viento,VtoSup10" EN="Wind speed at 10 meters." ES="Viento a 10 metros" 
			style="default-scalar" palette="wind2" ncWMStwo="true"
			name="wrf_dom2/Uat10:Vat10-mag" defParticleSpeed="0.03" overlaystreamlines="wrf_dom2/Uat10:wrf_dom2/Vat10"/>

            <layer Menu="Dom2,Viento,Vto250hP" EN="Wind speed at 250 hPa" ES="Viento en 250 hPa" 
			style="default-scalar" palette="wind2" 
			   name="wrf_dom2/Uat250:Vat250-mag" defParticleSpeed="0.03" overlaystreamlines="wrf_dom2/Uat250:wrf_dom2/Vat250"/>
	
            <layer Menu="Dom2,Viento,Vto500hP" EN="Wind speed in 500 hPa." ES="Viento en 500 hPa" 
			style="default-scalar" palette="wind2" 
			   name="wrf_dom2/Uat500:Vat500-mag" defParticleSpeed="0.03" overlaystreamlines="wrf_dom2/Uat500:wrf_dom2/Vat500"/>

            <layer Menu="Dom2,Viento,Vto700hP" EN="Wind speed in 700 hPa." ES="Viento en 700 hPa" 
			style="default-scalar" palette="wind2" 
			   name="wrf_dom2/Uat700:Vat700-mag" defParticleSpeed="0.03" overlaystreamlines="wrf_dom2/Uat700:wrf_dom2/Vat700" />

            <layer Menu="Dom2,Viento,Vto850hP" EN="Wind speed in 850 hPa" ES="Viento en 850 hPa" 
			style="default-scalar" palette="wind2" 
			   name="wrf_dom2/Uat850:Vat850-mag" defParticleSpeed="0.03" overlaystreamlines="wrf_dom2/Uat850:wrf_dom2/Vat850" />
            
            <layer Menu="Dom2,relh" EN="Relative humidity" ES="Humedad" name="wrf_dom2/RH" style="default-scalar" palette="seq-Blues" mincolor="0" maxcolor="100" />
            
            <layer Menu="Dom2,nubesIR" EN="Cloud coverage" ES="Nubes" name="wrf_dom2/QCLOUD" style="default-scalar" palette="seq-BlueHeat" mincolor="0" maxcolor="1" />
                
            <layer Menu="Dom2,vape" EN="Evaporation" ES="Evaporacion"  name="wrf_dom2/QFX" style="default-scalar" palette="seq-BlueHeat" mincolor="0" maxcolor="1" />
            
	</MainLayers>
	
</MenuConf>