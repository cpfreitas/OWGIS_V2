        <!-- ADCIRC -->
        <MainLayers BBOX="-122.25056107685108,7.409418707415154,-74.74943892314892,36.11315809004144" format="image/gif" 
                    server="http://132.248.8.238:8080/ncWMS_2015/wms" >
                   
		<layer Menu="ADCIRC,Pom,ele" EN="Water surface elevation above geoid (m)" ES="Elevación de la superficie del mar (m)" 
                    name="adcir_pom/zeta" ncWMStwo="true" palette="adcirc" mincolor="-3" maxcolor="3" belowMinColor="transparent" aboveMaxColor="transparent"
                />
                     
		<layer Menu="ADCIRC,Dom2,ele" EN="Water surface elevation above geoid (m)" ES="Elevación de la superficie del mar (m)" 
                    style="default-scalar" name="adcir_gom/zeta" ncWMStwo="true" palette="adcirc" mincolor="-3" maxcolor="3" belowMinColor="transparent" aboveMaxColor="transparent"
		/>                              
                
	</MainLayers>
        
        <!-- WWIII -->
        <!-- Pacifico -->
	<MainLayers BBOX="-180,90,-90,180" format="image/gif" ncWMStwo="true"
                    server="http://132.248.8.238:8080/ncWMS_2015/wms" >
                   
		<layer Menu="WWIII,Pom,Altura" EN="Significant wave height (m)" ES="Altura significante de oleaje (m)" 
                     name="wwiii_pom/hs" style="default-scalar" palette="alturamx" mincolor="0" maxcolor="12"
                     defParticleSpeed="0.03" overlaystreamlines="wwiii_pom/U:wwiii_pom/V" belowMinColor="transparent" aboveMaxColor="transparent"
                />                    
                
		<layer Menu="WWIII,Pom,Periodo" EN="Period" ES="Periodo" 
			style="default-scalar" palette="periodo" name="wwiii_pom/t01" mincolor="1" maxcolor="22"
                        defParticleSpeed="0.03" overlaystreamlines="wwiii_pom/U:wwiii_pom/V" belowMinColor="transparent" aboveMaxColor="transparent"
                />
                
	</MainLayers>
        
        <!-- Golfo -->
