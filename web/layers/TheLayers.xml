            <layer Menu="WRF,Dom1,relh" EN="Relative humidity" ES="Humedad Relativa" name="wrf_Dom1/RH" style="default-scalar" palette="rh" mincolor="50" belowMinColor="transparent" maxcolor="100" aboveMaxColor="transparent"/>
