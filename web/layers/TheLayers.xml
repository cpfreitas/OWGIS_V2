            <layer Menu="WRF,Dom1,relh,relh200" EN="Relative humidity 200hPa" ES="Humedad Relativa 200hPa" name="wrf_Dom1/RHat200" style="default-scalar" palette="rh" mincolor="50" belowMinColor="transparent" maxcolor="100" />
