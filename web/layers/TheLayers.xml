                    defParticleSpeed="0.01" overlaystreamlines="wwiii_wo/U:wwiii_wo/V"
                />                          
                
		<layer Menu="WWIII,Wo,Periodo" EN="Period" ES="Periodo" mincolor="1" maxcolor="22" belowMinColor="transparent" aboveMaxColor="transparent"
			style="default-scalar" palette="periodo" name="wwiii_wo/t01"
                        defParticleSpeed="0.01" overlaystreamlines="wwiii_wo/U:wwiii_wo/V"
                />
                
	</MainLayers>
        
