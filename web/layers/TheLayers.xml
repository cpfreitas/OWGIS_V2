                    style="default-scalar" palette="alturamx" name="wwiii_gom/hs" mincolor="0" maxcolor="12" belowMinColor="transparent" aboveMaxColor="transparent"
                    defParticleSpeed="0.03" overlaystreamlines="wwiii_gom/U:wwiii_gom/V"
                />                        
                
		<layer Menu="WWIII,Gom,Periodo" EN="Period" ES="Periodo" 
			style="default-scalar" palette="periodogo" name="wwiii_gom/t01" mincolor="0" maxcolor="13" belowMinColor="transparent" aboveMaxColor="transparent"
                        defParticleSpeed="0.03" overlaystreamlines="wwiii_gom/U:wwiii_gom/V"
                />
                
	</MainLayers>
        
        <!-- Global -->
        <MainLayers BBOX="-180,90,-90,180" format="image/gif" ncWMStwo="true"
                    server="http://132.248.8.238:8080/ncWMS_2015/wms" >
                   
