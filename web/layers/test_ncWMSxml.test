			   overlaystreamlines="hycom_forecast_uv/water_u:hycom_forecast_uv/water_v" />


		<!-- ncWMS with one date multiple Z-->
		<layer Menu="ncwms,ncwmsOneDateMultZ" 
			   EN="Demo of an ncWMS layer with only one day. World eastward wind."
