	</BackgroundLayers>
        -->
	<!-- Dominio 1 -->
	<MainLayers BBOX="-180,90,-90,180" format="image/gif" ncWMS="true"
                    server="http://132.248.8.238:8080/ncWMS_2015/wms" >
                   
		<layer Menu="Pom,Altura" EN="significant height of wind and swell waves (m)" ES="Altura significante del viento" 
                    style="boxfill" palette="prec" name="wwiii_pom/hs"
                />
                     
		<layer Menu="Pom,Viento" EN="Wind speed at 10 hPa" ES="Viento en 10 hPa" 
			style="boxfill" palette="speed" name="wwiii_pom/wind_ten_velocity" 
                    defParticleSpeed="0.03" overlaystreamlines="wwiii_pom/wind_ten_velocity"
                />                              
                
		<layer Menu="Pom,Periodo" EN="Period" ES="Periodo" 
			style="boxfill" palette="tempo" name="wwiii_pom/t01"
                />
                
	</MainLayers>
        
        <MainLayers BBOX="-180,90,-90,180" format="image/gif" ncWMS="true"
