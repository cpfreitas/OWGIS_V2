<MenuConf xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="schema.xsd">
	<!-- This are the menus that are available to display entries -->
	<Menus>
		<Menu ID="PromD" EN="Daily avarage" ES="Promedios Diarios"/>
		<Menu ID="PromM" EN="Monthly avarage" ES="Promedios Mensuales"/>
		<Menu ID="PromHM" EN="Month-hourly avarage" ES="Promedios Horarios Mensuales"/>
		<Menu ID="PromMAD" EN="AbsMax daily avarage" ES="Promedios Máximos Absolutos Diarios"/>
		<Menu ID="PromMAM" EN="AbsMax daily avarage" ES="Promedios Máximos Absolutos Mensuales"/>
		<Menu ID="Temp" EN="Temperature" ES="Temperatura"/>
		<Menu ID="Prec" EN="Prec" ES="Precipitación"/>
		<Menu ID="PBLH" EN="PBLH" ES="PBLH"/>
		<Menu ID="Viento" EN="Wind" ES="Viento"/>
		<Menu ID="SWDOWN" EN="SWDOWN" ES="SWDOWN"/>
		<Menu ID="GLW" EN="GLW" ES="GLW"/>
	</Menus>
        
        <BackgroundLayers BBOX="-180,90,-90,180" server="http://ncwms.coaps.fsu.edu/geoserver/wms">
		<layer name="comm:pyrResult512" featureInfo="comm:gebco"/> 
	</BackgroundLayers>

	<MainLayers BBOX="-122.25056107685108,7.409418707415154,-74.74943892314892,36.11315809004144" format="image/gif" server="http://132.248.8.238:8080/ncWMS_2015/wms" ncWMStwo="true" >  
                <!-- Promedios diarios -->
		<layer Menu="PromD,Temp" 
			   EN="Temperature" ES="Temperatura a 2 metros"
			   name="atlas_diario/T2" 
			   style="default-scalar" palette="temperatura"
                           mincolor="-15" maxcolor="50" belowMinColor="transparent" aboveMaxColor="transparent"
                />

		<layer Menu="PromD,Prec" 
			   EN="Total Precipitation" ES="Precipitación Total"
			   name="atlas_diario/PREC2" style="default-scalar"
			   mincolor="1" belowMinColor="transparent" maxcolor="500" aboveMaxColor="transparent" palette="precipitacionGrad"
                />

		<layer Menu="PromD,PBLH" 
			   EN="Boundary layer height" ES="Altura de la capa límite"
			   name="atlas_diario/PBLH" style="default-scalar"
                           mincolor="0" maxcolor="2000"
                />

		<layer Menu="PromD,Viento" 
			   EN="Wind speed at 10 meters" ES="Viento a 10 metros"  mincolor="0" maxcolor="70"
                           style="default-scalar" palette="wind2" 
			   name="atlas_diario/U10:V10-mag"  overlaystreamlines="atlas_diario/U10:atlas_diario/V10" defParticleSpeed="0.01"
                           belowMinColor="transparent" aboveMaxColor="transparent" 
                />

		<layer Menu="PromD,SWDOWN" EN="Shortwave radiation" 
			   ES="Radiación de onda corta" 
			   name="atlas_diario/SWDOWN" 
			   style="default-scalar" 
                />

		<layer Menu="PromD,GLW" EN="Longwave radiation" 
			   ES="Radiación de onda larga" 
			   name="atlas_diario/GLW" 
			   style="default-scalar" 
                />
                
