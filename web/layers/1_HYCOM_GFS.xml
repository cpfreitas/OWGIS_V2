<MenuConf xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="schema.xsd">
	<!--HYCOM-->
		 <!--localAddress="http://192.168.1.103:8080/ncWMS_2015/wms"-->
	<MainLayers BBOX="-180,-90,180,90"
				format="image/gif"  server="http://132.248.8.238:8080/ncWMS_2015/wms"
				ncWMS="true" max_time_range="month">  

		<layer Menu="hycom,cur" 
			   EN="Forecast currents"
			   name="hycom_forecast_uv/sea_water_velocity" 
			   defParticleSpeed=".9"
			   style="boxfill"
			   overlaystreamlines="hycom_forecast_uv/sea_water_velocity" />

		<layer Menu="hycom,ts" 
			   EN="Forecast temperature"
			   name="hycom_forecast_ts/water_temp" 
			   defParticleSpeed=".9"/>

		<layer Menu="hycom,sal" 
			   EN="Forecast salinity"
			   name="hycom_forecast_ts/salinity" 
			   palette="boxfill/redblue"
			   defParticleSpeed=".9"/>

		<layer Menu="hycom,ssh" 
			   EN="Forecast sea surface height"
			   name="hycom_forecast_ssh/surf_el" 
			   defParticleSpeed=".9"/>

	</MainLayers> 
</MenuConf>
